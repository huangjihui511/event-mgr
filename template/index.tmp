<h1>Hello! this is Jihui Event Mgr</h1>
<h2>System</h2>
<p>System time:{{$.Time}}</p>
<h2>Events</h2>
{{ range $index1, $item := $.Items}}
<h3>{{$item.Name}}</h3>
<p>Msg: {{$item.Msg}}</p>
<p>IsNotify: {{$item.IsNotify}}</p>
<p>LastCallAt: {{$item.LastCallAt}}</p>
<p>LastNotifyAt: {{$item.LastNotifyAt}}</p>
<p>Error: {{$item.Err}}</p>
{{end}}